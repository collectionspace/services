        xmlns:ns2="http://collectionspace.org/services/location">
        <displayName>${importLocationAuthorityWithVarExpansion.displayNameValue}</displayName>
        <shortIdentifier>${importLocationAuthorityWithVarExpansion.shortIdentifierValue}</shortIdentifier>
    </ns2:locationauthorities_common>
    <ns2:collectionspace_core xmlns:ns2="http://collectionspace.org/collectionspace_core/"
